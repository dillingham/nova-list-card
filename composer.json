{
    "name": "dillingham/nova-list-card",
    "description": "List Nova Resources",
    "keywords": [
        "palpalani",
        "laravel",
        "nova",
        "nova-list-card",
        "recent-list",
        "nova-resources"
    ],
    "homepage": "https://github.com/palpalani/nova-list-card",
    "license": "MIT",
    "authors": [
        {
            "name": "palPalani",
            "email": "palani.p@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "watson/aggregate": "^4.1|^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "NovaListCard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NovaListCard\\CardServiceProvider\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovaListCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
